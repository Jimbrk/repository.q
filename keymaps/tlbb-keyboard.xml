<keymap>
    <global>
        <keyboard name="The Little Black Box Remote">
            <power>ActivateWindow(ShutdownMenu)</power>                    <!-- Button 01 : Power -->
            <volume_mute>Mute</volume_mute>                                <!-- Button 02 : Mute -->
            <red>ActivateWindow(TVChannels)</red>                          <!-- Button 03 : Red -->
            <green>ActivateWindow(MyVideos)</green>                        <!-- Button 04 : Green -->
            <yellow>ActivateWindow(MyMusic)</yellow>                       <!-- Button 05 : Yellow -->
            <blue>ActivateWindow(MyPictures)</blue>                        <!-- Button 06 : Blue -->
            <comma>SkipPrevious</comma>                                    <!-- Button 07 : Previous -->
            <play_pause>PlayPause</play_pause>                             <!-- Button 08 : PlayPause -->
            <period>SkipNext</period>                                      <!-- Button 09 : Next -->
            <f6>RunScript(service.libreelec.settings)</f6>                 <!-- Button 10 : Wifi -->
            <rewind>Rewind</rewind>                                        <!-- Button 11 : Rewind -->
            <stop>Stop</stop>                                              <!-- Button 12 : Stop -->
            <fastforward>FastForward</fastforward>                         <!-- Button 13 : FastForward -->
            <menu>ActivateWindow(PlayerControls)</menu>                    <!-- Button 14 : MENU -->
            <f2>ActivateWindow(Music,musicdb://,return)</f2>               <!-- Button 15 : Music -->
            <f3>ActivateWindow(Videos,videodb://movies/,return)</f3>       <!-- Button 16 : Movies -->
            <f4>ActivateWindow(Videos,videodb://tvshows/,return)</f4>      <!-- Button 17 : TVShows -->
            <f5>ActivateWindow(Pictures,return)</f5>                       <!-- Button 18 : Pictures -->
            <tab>Fullscreen</tab>                                          <!-- Button 19 : TAB -->
            <favorites>ActivateWindow(Favourites)</favorites>              <!-- Button 20 : FAV -->
            <up>Up</up>                                                    <!-- Button 21 : Up -->
            <left>Left</left>                                              <!-- Button 22 : Left -->
            <return>Select</return>                                        <!-- Button 23 : Ok -->
            <right>Right</right>                                           <!-- Button 24 : Right -->
            <down>Down</down>                                              <!-- Button 25 : Down -->
            <config>ActivateWindow(Settings)</config>                      <!-- Button 26 : Settings -->
            <backspace>Back</backspace>                                    <!-- Button 27 : Back -->
            <volume_up>VolumeUp</volume_up>                                <!-- Button 28 : VOL+ -->
            <volume_down>VolumeDown</volume_down>                          <!-- Button 29 : VOL- -->
            <home>ActivateWindow(Home)</home>                              <!-- Button 30 : Home -->
            <f1>ContextMenu</f1>                                           <!-- Button 31 : ContextMenu -->
            <pageup>PageUp</pageup>                                        <!-- Button 32 : PG+ -->
            <pagedown>PageDown</pagedown>                                  <!-- Button 33 : PG- -->
            <one>Number1</one>                                             <!-- Button 34 : 1 -->
            <two>JumpSMS2</two>                                            <!-- Button 35 : 2 -->
            <three>JumpSMS3</three>                                        <!-- Button 36 : 3 -->
            <four>JumpSMS4</four>                                          <!-- Button 37 : 4 -->
            <five>JumpSMS5</five>                                          <!-- Button 38 : 5 -->
            <six>JumpSMS6</six>                                            <!-- Button 39 : 6 -->
            <seven>JumpSMS7</seven>                                        <!-- Button 30 : 7 -->
            <eight>JumpSMS8</eight>                                        <!-- Button 41 : 8 -->
            <nine>JumpSMS9</nine>                                          <!-- Button 42 : 9 -->
            <help>Info</help>                                              <!-- Button 43 : Info -->
            <zero>Number0</zero>                                           <!-- Button 44 : 0 -->
            <delete>Delete</delete>                                        <!-- Button 45 : BACK -->
        </keyboard>
    </global>
